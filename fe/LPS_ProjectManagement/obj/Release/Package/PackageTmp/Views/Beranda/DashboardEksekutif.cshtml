<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
    <section class="box margin-bottom-10 margin-top-5">
        <header class="panel_header">
            <h2 class="title pull-left">TOTAL TRANSFORMATION</h2>
        </header>
        <div class="content-body padding-bottom-0" style="max-height: 550px;">

            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="col-md-6" style="margin-right:-20px">
                        <label style="font-size:50px" @*id="totalProjectTransformasi"*@>@Model.Transformation</label><label style="font-size:20px; margin-left:5px">Project</label>
                    </div>

                    <div class="col-md-6">
                        <label style="font-size:40px" @*id="percentTransformasi"*@>(@Model.TransformationPercentage%)</label>
                    </div>
                    <div class="col-md-12 no-padding" style="margin-top:10px">
                        <label style="font-size:15px">Pencapaian</label>
                    </div>
                    <div class="col-md-12 no-padding" style="margin-top:0px">
                        <div class="col-md-5">
                            <label style="font-size:15px">Transformation :</label>
                        </div>
                        <div class="col-md-7 no-padding">
                            <div class="progress small">
                                @if (Convert.ToDouble(Model.ProgressTransfomation) >= 90)
                                {
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="@Convert.ToDouble(Model.ProgressTransfomation)"
                                         aria-valuemin="0" aria-valuemax="100" style="width:@Convert.ToDouble(Model.ProgressTransfomation)%;">
                                        @Convert.ToDouble(Model.ProgressTransfomation)%
                                    </div>
                                }
                                else
                                {
                                    <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="@Convert.ToDouble(Model.ProgressTransfomation)"
                                         aria-valuemin="0" aria-valuemax="100" style="width:@Convert.ToDouble(Model.ProgressTransfomation)%;">
                                        @Convert.ToDouble(Model.ProgressTransfomation)%
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

