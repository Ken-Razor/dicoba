<script>

    $.ajax({
        type: "GET",
        url: '@Url.Action("GetDashboardEksekutif", "Beranda")',
        data: {},
    traditional: true,
        success: function (success) {
            debugger;
            pieChartData(success.ListStatusProject);
        }
    });

    function pieChartData(data) {
        var i;
        var dataArray = [];
        for (i = 0; i < data.length; i++) {
                dataArray.push({ name: data[i].StatusName, y: data[i].Total });
        }

